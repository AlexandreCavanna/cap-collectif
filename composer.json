        {
            "type": "vcs",
            "url": "https://github.com/cap-collectif/behat-parallel-scenario"
        },
        {
            "type": "vcs",
            "url": "https://github.com/spyl94/GraphQLBundle"
        },
        {
            "type": "vcs",
            "url": "https://github.com/cap-collectif/BehatPageObjectExtension"
        },
        {
            "type": "vcs",
            "url": "https://github.com/Liinkiing/geoPHP"
        },
        {
            "type": "vcs",
            "url": "https://github.com/cap-collectif/mailcatcher"
        },
        {
            "type": "vcs",
            "url": "https://github.com/berretes/ExcelBundle"
        },
        {
            "type": "vcs",
            "url": "https://github.com/cap-collectif/HWIOAuthBundle"
