    "autoload": {
        "exclude-from-classmap": [
            "**/Behat/"
        ],
        "psr-4": {
            "spec\\": "spec/",
            "Capco\\": "src/",
            "Capco\\AppBundle\\": "src/Capco/AppBundle",
            "Capco\\AdminBundle\\": "src/Capco/AdminBundle",
            "Capco\\ClassificationBundle\\": "src/Capco/ClassificationBundle",
            "Capco\\MediaBundle\\": "src/Capco/MediaBundle",
            "Capco\\UserBundle\\": "src/Capco/UserBundle",
            "Capco\\AppBundle\\GraphQL\\__GENERATED__\\": "src/Capco/AppBundle/GraphQL/__generated__"
        },
        "files": [
            "src/Kernel.php",
            "src/functions.php"
        ]
    },
